<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!DOCTYPE html>
<html>
<head>
    <title>request JSP</title>
</head>
<body>
    <h1>Hello, JSP!</h1>
    <%
        String name = request.getParameter("name");
        String age = request.getParameter("age");
    %>
    <p>Name: <%= name %></p>
    <p>Age: <%= age %></p>
</body>
</html>
